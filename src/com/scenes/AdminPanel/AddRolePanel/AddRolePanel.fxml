<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="150.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.scenes.AdminPanel.AddRolePanel.AddRoleController">
    <children>
        <TextField fx:id="roleField" layoutX="50.0" layoutY="55.0"/>
        <Label layoutX="50.0" layoutY="37.0" text="New role">
            <font>
                <Font name="Tahoma" size="14.0"/>
            </font>
        </Label>
        <Button onAction="#submit" layoutX="90.0" layoutY="100.0" mnemonicParsing="false" text="Add role">
            <font>
                <Font name="Tahoma" size="14.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
